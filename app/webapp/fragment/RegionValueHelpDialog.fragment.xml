<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Region"
		ok=".onValueHelpOkPressRegion"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportRanges="true"
		key="RegionID"
		descriptionKey="RegionName">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="RegionID"
						label="Region ID"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="RegionID" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="RegionName"
						label="Region Name"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="RegionName" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
